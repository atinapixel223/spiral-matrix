class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class Solution:
    def sortedInsert(self, head, key):
        new_node = Node(key)

        if head is None or key < head.data:
            new_node.next = head
            return new_node

        curr = head
        while curr.next is not None and curr.next.data < key:
            curr = curr.next

        new_node.next = curr.next
        curr.next = new_node

        return head
